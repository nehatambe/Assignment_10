{
    "env": {
        "browser": true
    },
    "parserOptions":{
      "sourceType":"module"
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console":0,
        "no-undef":0,
        "no-unresolved":0,
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
